<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="profilePageVM" type="com.juansapplication.app.modules.profilepage.data.viewmodel.ProfilePageVM"/>
    </data>
    <FrameLayout android:id="@+id/frameProfilePage"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylewhite_A700">
        <LinearLayout android:id="@+id/linearColumnuser"
            android:layout_gravity="start"
            android:layout_height="match_parent"
            android:layout_width="match_parent" android:orientation="vertical">
            <LinearLayout android:gravity="end"
                android:id="@+id/linearRowuser"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylebluegray_100">
                <ImageView android:id="@+id/imageUser"
                    android:layout_height="@dimen/_81pxh"
                    android:layout_marginBottom="@dimen/_37pxv"
                    android:layout_marginStart="@dimen/_42pxh"
                    android:layout_marginTop="@dimen/_36pxv"
                    android:layout_width="@dimen/_84pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user_red_a700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user_red_a700"/>
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnname"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_28pxv"
                    android:layout_marginEnd="@dimen/_6pxh"
                    android:layout_marginStart="@dimen/_28pxh"
                    android:layout_marginTop="@dimen/_36pxv"
                    android:layout_weight="0.7"
                    android:layout_width="0dp" android:orientation="vertical">
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnName"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_name"
                        android:textAllCaps="true"
                        style="@style/btnRoundedOutline" tools:text="@string/lbl_name"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnBio"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:lineSpacingExtra="@dimen/_4pxh"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_bio"
                        android:textAllCaps="true"
                        style="@style/btnRoundedOutline" tools:text="@string/lbl_bio"/>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearFriends"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_77pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylecornerRadius">
                <ImageView android:id="@+id/imageUserOne"
                    android:layout_height="@dimen/_15pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginStart="@dimen/_25pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_width="@dimen/_13pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_user"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_user"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFriends"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginEnd="@dimen/_120pxh"
                    android:layout_marginStart="@dimen/_77pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_weight="0.83"
                    android:layout_width="0dp"
                    android:lineSpacingExtra="@dimen/_4pxh"
                    android:singleLine="true"
                    android:text="@{profilePageVM.profilePageModel.txtFriends}"
                    android:textAllCaps="true"
                    style="@style/txtMontserratlight14" tools:text="@string/lbl_friends"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleft"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_9pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleft"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft_indigo_400"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_indigo_400"/>
                <View
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_31_5"
                    android:id="@+id/viewEllipseTwo"
                    android:layout_height="@dimen/_59pxv"
                    android:layout_marginStart="@dimen/_10pxh" android:layout_width="@dimen/_63pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_31_5"
                    android:id="@+id/viewEllipseThree"
                    android:layout_height="@dimen/_59pxv"
                    android:layout_marginStart="@dimen/_43pxh" android:layout_width="@dimen/_63pxh"/>
                <View
                    android:background="@drawable/rectangle_bg_bluegray_100_radius_31_5"
                    android:id="@+id/viewEllipseFour"
                    android:layout_height="@dimen/_59pxv"
                    android:layout_marginStart="@dimen/_43pxh" android:layout_width="@dimen/_63pxh"/>
                <ImageView android:id="@+id/imageArrowright"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginBottom="@dimen/_10pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_indigo_400"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_indigo_400"/>
            </LinearLayout>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnGames"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_13pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_71pxv"
                android:layout_width="match_parent"
                android:lineSpacingExtra="@dimen/_4pxh"
                android:paddingBottom="@dimen/_13pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_13pxv"
                android:singleLine="true"
                android:text="@string/lbl_games"
                android:textAllCaps="true"
                style="@style/btnRoundedOutline" tools:text="@string/lbl_games"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearRowarrowleftOne"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_9pxh"
                android:layout_marginStart="@dimen/_13pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageArrowleftOne"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowleft_indigo_400"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowleft_indigo_400"/>
                <View android:background="@color/bluegray_100"
                    android:id="@+id/viewRectangleFour"
                    android:layout_height="@dimen/_65pxv"
                    android:layout_marginStart="@dimen/_6pxh" android:layout_width="@dimen/_72pxh"/>
                <View android:background="@color/bluegray_100"
                    android:id="@+id/viewRectangleFive"
                    android:layout_height="@dimen/_65pxv"
                    android:layout_marginStart="@dimen/_34pxh" android:layout_width="@dimen/_72pxh"/>
                <View android:background="@color/bluegray_100"
                    android:id="@+id/viewRectangleSix"
                    android:layout_height="@dimen/_65pxv"
                    android:layout_marginStart="@dimen/_34pxh" android:layout_width="@dimen/_72pxh"/>
                <ImageView android:id="@+id/imageArrowrightOne"
                    android:layout_height="@dimen/_40pxh"
                    android:layout_marginBottom="@dimen/_12pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_13pxv"
                    android:layout_width="@dimen/_19pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_arrowright_indigo_400"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_arrowright_indigo_400"/>
            </LinearLayout>
            <LinearLayout android:id="@+id/linearNavBar"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_50pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylebluegray_100">
                <FrameLayout android:id="@+id/frameStackhome"
                    android:layout_height="@dimen/_38pxv"
                    android:layout_width="@dimen/_90pxh" style="@style/groupStylebluegray_101">
                    <ImageView android:id="@+id/imageHome"
                        android:layout_gravity="top|center"
                        android:layout_height="@dimen/_26pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_25pxh"
                        android:layout_marginStart="@dimen/_25pxh"
                        android:layout_marginTop="@dimen/_4pxv"
                        android:layout_width="@dimen/_38pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_home"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_home"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStackmaximize"
                    android:layout_height="@dimen/_38pxv"
                    android:layout_width="@dimen/_90pxh" style="@style/groupStylebluegray_101">
                    <ImageView android:id="@+id/imageMaximize"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_31pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginEnd="@dimen/_23pxh"
                        android:layout_marginStart="@dimen/_23pxh"
                        android:layout_marginTop="@dimen/_3pxv"
                        android:layout_width="@dimen/_33pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_maximize"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_maximize"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStacksearch"
                    android:layout_height="@dimen/_38pxv"
                    android:layout_width="@dimen/_90pxh" style="@style/groupStylebluegray_101">
                    <ImageView android:id="@+id/imageSearch"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_32pxh"
                        android:layout_marginBottom="@dimen/_3pxv"
                        android:layout_marginEnd="@dimen/_24pxh"
                        android:layout_marginStart="@dimen/_24pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="@dimen/_36pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_search"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
                    <ImageView android:id="@+id/imageUserTwo"
                        android:layout_gravity="top|start"
                        android:layout_height="@dimen/_20pxh"
                        android:layout_marginBottom="@dimen/_10pxv"
                        android:layout_marginEnd="@dimen/_31pxh"
                        android:layout_marginStart="@dimen/_31pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_22pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user_gray_900"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user_gray_900"/>
                </FrameLayout>
                <FrameLayout android:id="@+id/frameStackuserThree"
                    android:layout_height="@dimen/_38pxv"
                    android:layout_width="@dimen/_90pxh" style="@style/groupStylebluegray_101">
                    <ImageView android:id="@+id/imageUserThree"
                        android:layout_gravity="end"
                        android:layout_height="@dimen/_27pxh"
                        android:layout_marginBottom="@dimen/_4pxv"
                        android:layout_marginEnd="@dimen/_26pxh"
                        android:layout_marginStart="@dimen/_26pxh"
                        android:layout_marginTop="@dimen/_5pxv"
                        android:layout_width="@dimen/_30pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_user_gray_900_27x30"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_user_gray_900_27x30"/>
                </FrameLayout>
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>
</layout>
